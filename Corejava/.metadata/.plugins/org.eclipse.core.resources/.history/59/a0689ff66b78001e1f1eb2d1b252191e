package in.com.Array;

public class Testargumentpassing2 {
	
	public static void main(String[] args) {
		String a = (args[0]);
		String b = (args[1]);
	
	System.out.println(a);
		
	
		
		int a = Integer.parseInt(args[0]);
		int b = Integer.parseInt(args[1]);
		int sum = a+b;
		System.out.println(sum);
		
	}

}
